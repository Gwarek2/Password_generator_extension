{
    "name": "Password generator",
    "version": "0.5",
    "description": "Simple password generator.\n",
    "details": "Hello",
    "applications": {
        "gecko": {
            "id": "pass-gen@mozilla.org",
            "strict_min_version": "45.0"
        }
    },
    "permissions": ["declarativeContent"], 
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon.png",
            "32": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        }
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "manifest_version": 2
}
